{
  "name": "imlazy",
  "version": "0.0.0-development",
  "description": "Functional programming with lazy immutable iterables",
  "main": "index.js",
  "scripts": {
    "benchmark": "node benchmarks/transducers-and-native",
    "docs-watch": "nodemon --watch index.js --watch docsSrc -e css -e js docsSrc/docs",
    "docs": "node docsSrc/docs",
    "spec-watch": "tape-watch 'test/**/*.js' | faucet",
    "spec": "tape 'test/**/*.js' | faucet",
    "test-watch": "nodemon --exec npm t",
    "test": "npm run test:lint && npm run spec",
    "test:lint": "eslint .",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benji6/imlazy.git"
  },
  "author": "Ben Hall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benji6/imlazy/issues"
  },
  "keywords": [
    "circular",
    "data",
    "declarative",
    "functional",
    "immutable",
    "infinite",
    "iterable",
    "iteration",
    "iterator",
    "lazy",
    "protocol",
    "sequence",
    "structure"
  ],
  "homepage": "https://github.com/benji6/imlazy",
  "dependencies": {
    "lodash.isequalwith": "^4.4.0"
  },
  "devDependencies": {
    "benchmark": "^2.1.0",
    "dox": "^0.9.0",
    "eslint": "^5.11.0",
    "eslint-config-benji6": "^1.0.0",
    "faucet": "^0.0.1",
    "fun-task": "^1.5.2",
    "highlight.js": "^9.13.1",
    "html-minifier": "^3.5.21",
    "immutable": "^4.0.0-rc.12",
    "nodemon": "^1.18.9",
    "ramda": "^0.26.1",
    "semantic-release": "^15.13.1",
    "tape": "^4.9.1",
    "tape-watch": "^2.3.0"
  }
}
