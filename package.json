{
  "name": "imlazy",
  "version": "6.1.0",
  "description": "Functional programming with lazy immutable iterables",
  "main": "index.js",
  "scripts": {
    "benchmark": "node benchmark",
    "docs": "jsdoc index.js -d docs",
    "lint": "eslint .",
    "spec": "ava",
    "test": "concurrently 'npm run lint' 'npm run spec'",
    "watch-docs": "nodemon --watch index.js --exec npm run docs",
    "watch-spec": "nodemon --exec npm run spec",
    "watch-test": "nodemon --exec npm t"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benji6/imlazy.git"
  },
  "author": "Ben Hall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benji6/imlazy/issues"
  },
  "keywords": [
    "circular",
    "data",
    "declarative",
    "functional",
    "immutable",
    "infinite",
    "iterable",
    "iteration",
    "iterator",
    "lazy",
    "protocol",
    "sequence",
    "structure"
  ],
  "homepage": "https://github.com/benji6/imlazy",
  "devDependencies": {
    "ava": "^0.17.0",
    "benchmark": "^2.1.0",
    "concurrently": "^3.1.0",
    "eslint": "^3.14.0",
    "eslint-config-benji6": "^0.5.0",
    "immutable": "^3.8.1",
    "jsdoc": "^3.4.0",
    "nodemon": "^1.9.2",
    "ramda": "^0.23.0"
  },
  "dependencies": {
    "lodash.isequalwith": "^4.4.0"
  }
}
