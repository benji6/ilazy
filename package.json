{
  "name": "imlazy",
  "version": "0.0.0-development",
  "description": "Functional programming with lazy immutable iterables",
  "main": "index.js",
  "scripts": {
    "benchmark": "node benchmarks/transducers-and-native",
    "docs-watch": "nodemon --watch index.js --watch docsSrc -e css -e js docsSrc/docs",
    "docs": "node docsSrc/docs",
    "lint": "eslint .",
    "spec-watch": "tape-watch 'test/**/*.js' | faucet",
    "spec": "tape 'test/**/*.js' | faucet",
    "test-watch": "nodemon --exec npm t",
    "test": "npm run lint && npm run spec",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benji6/imlazy.git"
  },
  "author": "Ben Hall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benji6/imlazy/issues"
  },
  "keywords": [
    "circular",
    "data",
    "declarative",
    "functional",
    "immutable",
    "infinite",
    "iterable",
    "iteration",
    "iterator",
    "lazy",
    "protocol",
    "sequence",
    "structure"
  ],
  "homepage": "https://github.com/benji6/imlazy",
  "dependencies": {
    "lodash.isequalwith": "^4.4.0"
  },
  "devDependencies": {
    "benchmark": "^2.1.0",
    "dox": "^0.9.0",
    "eslint": "^4.19.1",
    "eslint-config-benji6": "^1.0.0",
    "faucet": "^0.0.1",
    "fun-task": "^1.5.2",
    "highlight.js": "^9.9.0",
    "html-minifier": "^3.5.16",
    "immutable": "^3.8.1",
    "nodemon": "^1.17.5",
    "ramda": "^0.24.0",
    "semantic-release": "^15.1.4",
    "tape": "^4.9.1",
    "tape-watch": "^2.3.0",
    "travis-deploy-once": "^4.4.1"
  }
}
