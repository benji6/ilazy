{
  "name": "imlazy",
  "version": "0.0.0-development",
  "description": "Functional programming with lazy immutable iterables",
  "main": "index.js",
  "scripts": {
    "benchmark": "node benchmarks/transducers-and-native",
    "docs-watch": "nodemon --watch index.js --watch docsSrc -e css -e js docsSrc/docs",
    "docs": "node docsSrc/docs",
    "format": "prettier --write '**/*'",
    "test-watch": "tape-watch 'test/**/*.js' | faucet",
    "test": "run-p test:*",
    "test:audit": "yarn audit --level critical || [ $? -lt 16 ]",
    "test:format": "prettier --check '**/*'",
    "test:unit": "tape 'test/**/*.js' | faucet",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benji6/imlazy.git"
  },
  "author": "Ben Hall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benji6/imlazy/issues"
  },
  "keywords": [
    "circular",
    "data",
    "declarative",
    "functional",
    "immutable",
    "infinite",
    "iterable",
    "iteration",
    "iterator",
    "lazy",
    "protocol",
    "sequence",
    "structure"
  ],
  "homepage": "https://github.com/benji6/imlazy",
  "dependencies": {
    "lodash.isequalwith": "^4.4.0"
  },
  "devDependencies": {
    "benchmark": "^2.1.0",
    "dox": "^0.9.0",
    "faucet": "^0.0.1",
    "fun-task": "^1.5.2",
    "highlight.js": "^9.13.1",
    "html-minifier": "^4.0.0",
    "immutable": "^4.0.0-rc.12",
    "nodemon": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ramda": "^0.26.1",
    "semantic-release": "^17.0.3",
    "tape": "^4.9.1",
    "tape-watch": "^2.3.0"
  }
}
